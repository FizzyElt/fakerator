{
  "name": "struct-fakerator",
  "version": "2.3.4",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "module": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./utils/*": {
      "default": {
        "types": "./dist/utils/*.d.ts",
        "default": "./dist/utils/*.mjs"
      },
      "require": {
        "types": "./dist/utils/*.d.ts",
        "default": "./dist/utils/*.js"
      },
      "import": {
        "types": "./dist/utils/*.d.ts",
        "default": "./dist/utils/*.mjs"
      },
      "module": {
        "types": "./dist/utils/*.d.ts",
        "default": "./dist/utils/*.mjs"
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "publish": "pnpm build && pnpm publish --access public",
    "test": "vitest src"
  },
  "keywords": [],
  "author": "FizzyElt",
  "homepage": "https://github.com/FizzyElt/fakerator",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "^1.9.1",
    "@faker-js/faker": "^9.0.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "publishConfig": {
    "access": "public"
  }
}